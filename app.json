{
  "expo": {
    "name": "DoamarApp",
    "slug": "DoamarApp",
    "version": "1.0.0",
    "web": {
      "favicon": "./src/assets/global/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./src/assets/global/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/global/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/global/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
